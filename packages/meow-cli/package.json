{
  "name": "meow-cli",
  "version": "0.12.1",
  "description": "CLI for meow",
  "keywords": [
    "zChain",
    "meow"
  ],
  "license": "(Apache-2.0 OR MIT)",
  "exports": "./build/src/index.js",
  "types": "build/src/index.d.ts",
  "preferGlobal": true,
  "type": "module",
  "engines": {
    "node": ">=14.13.1 || >=16.0.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zer0-os/zChain"
  },
  "bin": {
    "meow": "build/src/cli.js"
  },
  "scripts": {
    "test": "npm run test:node",
    "build": "tsc --build .",
    "build:watch": "tsc -w -p .",
    "meow": "meow"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.2",
    "@ipld/dag-json": "^8.0.10",
    "@ipld/dag-pb": "^2.1.17",
    "debug": "^4.3.4",
    "err-code": "^3.0.1",
    "ipfs": "^0.63.1",
    "ipfs-core-types": "^0.11.0",
    "ipfs-daemon": "^0.13.1",
    "ipfs-http-client": "^57.0.0",
    "it-all": "^1.0.6",
    "it-glob": "^1.0.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.10",
    "sinon": "^14.0.0"
  }
}
