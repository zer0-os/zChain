{
  "name": "meow-cli",
  "version": "0.12.1",
  "description": "CLI for meow",
  "keywords": [
    "zChain",
    "meow"
  ],
  "license": "(Apache-2.0 OR MIT)",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "preferGlobal": true,
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zer0-os/zChain"
  },
  "bin": {
    "meow": "build/src/cli.js"
  },
  "scripts": {
    "test": "npm run test:node",
    "build": "tsc --build .",
    "build:watch": "tsc -w -p .",
    "meow": "meow"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.1",
    "@ipld/dag-json": "^8.0.9",
    "@ipld/dag-pb": "^2.1.16",
    "debug": "^4.1.1",
    "err-code": "^3.0.1",
    "ipfs": "^0.62.3",
    "ipfs-core-types": "^0.10.3",
    "ipfs-daemon": "^0.12.3",
    "ipfs-http-client": "^56.0.3",
    "it-all": "^1.0.6",
    "it-glob": "^1.0.2",
    "yargs": "^16.0.3",
    "meow-app": "^1.0.0"
  },
  "devDependencies": {
    "@types/yargs": "^16.0.0",
    "peer-id": "^0.16.0",
    "sinon": "^12.0.0"
  }
}
